{
	"CrawlerSettings": {
		"BaseUrl": "http://seobravo.co.kr/",
		"IsPathBasedPagination": true,
		"PathPageFormat": "page/{0}",
		"QueryPageFormat": "page={0}",
		"StartPage": 1,
		"DetectLastPage": true,
		"MaxPages": 10,
		"DelayMilliseconds": 1500,
		"OutputDirectory": "data",
		"LastPagePattern": "//div[@class='nav-links']/a[@class='next page-numbers']/parent::div//a[contains(@class, 'page-numbers') and not(contains(@class, 'next'))]",
		"AppendDateTime": true,

		"Boards": [
			{
				"Name": "이목사의창칼럼",
				"Url": "http://seobravo.co.kr/category/%ec%9d%b4%eb%aa%a9%ec%82%ac%ec%9d%98%ec%b0%bd/%ec%9d%b4%eb%aa%a9%ec%82%ac%ec%9d%98%ec%b0%bd%ec%b9%bc%eb%9f%bc/",
				"ItemSelector": "//article[contains(@class, 'post')]",
				"TitleSelector": ".//h2[@class='entry-title']/a",
				"ContentSelector": ".//div[@class='entry-content']",
				"PaginationSelector": "//nav[@class='navigation pagination']"
			},
			{
				"Name": "게시판",
				"Url": "http://seobravo.co.kr/category/%ea%b2%8c%ec%8b%9c%ed%8c%90/",
				"ItemSelector": "//article[contains(@class, 'post')]",
				"TitleSelector": ".//h2[@class='entry-title']/a",
				"ContentSelector": ".//div[@class='entry-content']",
				"PaginationSelector": "//nav[@class='navigation pagination']"
			}
		]
	}
}